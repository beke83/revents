ent={event.title} />\r\n              <Item.Description>Hosted by {event.hostedBy}</Item.Description>\r\n              {event.isCancelled && (\r\n                <Label style={{top: '-40px'}} \r\n                ribbon='right'\r\n                color='red'\r\n                content='This event is cancelled'\r\n                />\r\n              )}\r\n            </Item.Content>\r\n          </Item>\r\n        </Item.Group>\r\n      </Segment>\r\n      <Segment>\r\n        <span>\r\n          <Icon name='clock' />\r\n          {format(event.date, \"MMMM d, yyyy h:mm a\")}\r\n          <Icon name='marker' />\r\n          {event.venue}\r\n        </span>\r\n      </Segment>\r\n      <Segment secondary>\r\n        <List horizontal>\r\n          {event.attendees.map((attendee) => (\r\n            <EventListAttendee key={attendee.id} attendee={attendee} />\r\n          ))}\r\n        </List>\r\n      </Segment>\r\n      <Segment clearing>\r\n        <div>{event.description}</div>\r\n        <Button\r\n          onClick={() => deleteEventInFirestore(event.id)}\r\n          color='red'\r\n          floated='right'\r\n          content='Delete'\r\n        ></Button>\r\n        <Button\r\n          //used backtick to allow adding javascript to specify the particular event\r\n          as={Link}\r\n          to={`/events/${event.id}`}\r\n          color='teal'\r\n          floated='right'\r\n          content='View'\r\n        ></Button>\r\n      </Segment>\r\n    </Segment.Group>\r\n  );\r\n}\r\n","C:\\Users\\beke\\Documents\\Projects\\revents\\src\\features\\events\\eventDashboard\\EventListAttendee.jsx",[],"C:\\Users\\beke\\Documents\\Projects\\revents\\src\\features\\events\\eventForm\\EventForm.jsx",[],"C:\\Users\\beke\\Documents\\Projects\\revents\\src\\app\\api\\sampleData.js",[],"C:\\Users\\beke\\Documents\\Projects\\revents\\src\\features\\home\\HomePage.jsx",["226"],"import React from \"react\";\r\nimport { NavLink, useHistory } from \"react-router-dom\";\r\nimpor